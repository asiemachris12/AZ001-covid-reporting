{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF001-covid-reporting"
		},
		"LS001_blob_covidreporting_SA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS001_blob_covidreporting_SA'"
		},
		"LS001_covidreporting_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS001_covidreporting_dl'"
		},
		"LS001_covidreporting_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dl002covidreporting.dfs.core.windows.net/"
		},
		"LS003_HTTP_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF001-covid-reporting_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS001_blob_covidreporting_SA_connectionString": {
						"value": "[parameters('LS001_blob_covidreporting_SA_connectionString')]"
					},
					"LS001_covidreporting_dl_accountKey": {
						"value": "[parameters('LS001_covidreporting_dl_accountKey')]"
					},
					"LS001_covidreporting_dl_properties_typeProperties_url": {
						"value": "[parameters('LS001_covidreporting_dl_properties_typeProperties_url')]"
					},
					"LS003_HTTP_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('LS003_HTTP_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}